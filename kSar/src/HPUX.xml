    Description:
        Configure color of graphs

-->

<ConfiG>
   
    <!--  BEGIN OS -->
    <OS>
        <OSType name="HP-UX">
            <Stat name="cpu1">
                <headerstr>%usr %sys %wio %idle</headerstr>
                <graphname>CPU</graphname>
            </Stat>
            <Stat name="device1">
                <headerstr>device %busy avque r+w/s blks/s avwait avserv</headerstr>
                <graphname>DEVICE</graphname>
            </Stat>
            <Stat name="runq">
                <headerstr>runq-sz %runocc swpq-sz %swpocc</headerstr>
                <graphname>RUNQ</graphname>
            </Stat>
            <Stat name="buffer">
                <headerstr>bread/s lread/s %rcache bwrit/s lwrit/s %wcache pread/s pwrit/s</headerstr>
                <graphname>BUFFER</graphname>
            </Stat>
            <Stat name="swap">
                <headerstr>swpin/s bswin/s swpot/s bswot/s pswch/s</headerstr>
                <graphname>SWAP</graphname>
            </Stat>
            <Stat name="syscall">
                <headerstr>scall/s sread/s swrit/s fork/s exec/s rchar/s wchar/s</headerstr>
                <graphname>SYSCALL</graphname>
            </Stat>
            <Stat name="file">
                <headerstr>iget/s namei/s dirbk/s</headerstr>
                <graphname>FILE</graphname>
            </Stat>
            <Stat name="tty">
                <headerstr>rawch/s canch/s outch/s rcvin/s xmtin/s mdmin/s</headerstr>
                <graphname>TTY</graphname>
            </Stat>
            <Stat name="ksize">
                <headerstr>text-sz ov proc-sz ov inod-sz ov file-sz ov</headerstr>
                <graphname>IGNORE</graphname>
            </Stat>
            <Stat name="msgs">
                <headerstr>msg/s sema/s</headerstr>
                <graphname>MSGS</graphname>
            </Stat>
            <Graph name="CPU" Title="CPU" type="unique">
                <Stack Title="Used" size="3">%usr %sys %wio</Stack>
                <Plot Title="Idle">%idle</Plot>
            </Graph>
            <Graph name="DEVICE" Title="Device" type="multiple">
                <Plot Title="busy">%busy</Plot>
                <Plot Title="avque">avque</Plot>
                <Plot Title="r+w/s">r+w/s</Plot>
                <Plot Title="blks/s">blks/s</Plot>
                <Plot Title="avwait">avwait</Plot>
                <Plot Title="avserv">avserv</Plot>
            </Graph>
            <Graph name="RUNQ" Title="RunQ" type="unique">
                <Plot Title="runQ">runq-sz</Plot>
                <Plot Title="swapQ">swpq-sz</Plot>
            </Graph>
            <Graph name="BUFFER" Title="Buffer" type="unique">
                <Plot Title="read">bread/s lread/s</Plot>
                <Plot Title="write">bwrit/s lwrit/s</Plot>
                <Plot Title="physical">pread/s pwrit/s</Plot>
                <Plot Title="%">%rcache %wcache</Plot>
            </Graph>
            <Graph name="SWAP" Title="Swap" type="unique">
                <Plot Title="lwp">swpin/s swpot/s</Plot>
                <Plot Title="page">bswin/s bswot/s</Plot>
                <Plot Title="switch">pswch/s</Plot>
            </Graph>
            <Graph name="SYSCALL" Title="Syscal" type="unique">
                <Plot Title="syscall">scall/s</Plot>
                <Plot Title="r/w">sread/s swrit/s</Plot>
                <Plot Title="fork">fork/s exec/s</Plot>
                <Plot Title="char">rchar/s wchar/s</Plot>
            </Graph>
            <Graph name="FILE" Title="File" type="unique">
                <Plot Title="file">iget/s namei/s dirbk/s</Plot>
            </Graph>
            <Graph name="TTY" Title="Tty" type="unique">
                <Plot Title="TTY">rawch/s canch/s outch/s rcvin/s xmtin/s mdmin/s</Plot>
            </Graph>
            <Graph name="MSGS" Title="Messages / Semaphores" type="unique">
                <Plot Title="Messages">msg/s</Plot>
                <Plot Title="Semaphores">sema/s</Plot>
            </Graph>
        </OSType>
    </OS>
    <!--  END OS -->
</ConfiG>

